{
    "name": "pressor/framework",
    "description": "Wordpress as a composer dependency in the context of a Laravel appliction",
    "keywords": ["framework", "laravel", "wordpress", "pressor"],
    "license": "MIT",
    "authors": [
        {
            "name": "Austin Wei"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "laravel/framework": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "0.9.*",
        "unit-testing/mockery-helper": "dev-master",
        "unit-testing/function-spy": "dev-master",
        "unit-testing/class-spy": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Pressor\\" : "src"
        }
    }
}
